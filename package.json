{
  "name": "typescript-sequelize",
  "version": "1.0.0",
  "description": "API desenvolvida utilizando TypeScript, Node.js, ES6, MSSQL e Sequelize",
  "main": "./src/server.ts",
  "scripts": {
    "start": "NODE_ENV=development ./node-modules/.bin/ts-node ./server/server.ts",
    "builded": "NODE_ENV=development node ./build/debug/server.js",
    "unit-test": "NODE_ENV=text mocha --opts build/debug/tests/unit/config/mocha.opts ./build/debug/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts build/debug/tests/integration/config/mocha.opts ./build/debug/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiagopsilva/node-typescript-sequelize.git"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "ES6",
    "Sequelize"
  ],
  "engines": {
    "node": "~8.2.1",
    "npm": "~5.5.1"
  },
  "author": "Tiago Silva (tiago.prs@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tiagopsilva/node-typescript-sequelize/issues"
  },
  "homepage": "https://github.com/tiagopsilva/node-typescript-sequelize#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "http": "0.0.0",
    "morgan": "^1.9.0",
    "mssql": "^4.1.0",
    "sequelize": "^4.22.6",
    "sequelize-cli": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.39",
    "@types/mocha": "^2.2.44",
    "@types/morgan": "^1.7.35",
    "@types/sequelize": "^4.0.79",
    "@types/supertest": "^2.0.3",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.6",
    "ts-node": "^3.3.0"
  }
}
